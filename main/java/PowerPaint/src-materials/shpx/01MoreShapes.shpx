	<shape name="Hexagonal Prism">
		<param name="a"
		       origin-x="0" origin-y="-0.1"
		       min-x="0" min-y="-0.1"
		       def-x="0.25" def-y="-0.1"
		       max-x="0.5" max-y="-0.1"/>
		<param name="b"
		       origin-x="-0.1" origin-y="0"
		       min-x="-0.1" min-y="0"
		       def-x="-0.1" def-y="0.25"
		       max-x="-0.1" max-y="0.5"/>
		<polygon points="a.x 0 1-a.x 0 1 b.y 1-a.x 2*b.y a.x 2*b.y 0 b.y"/>
		<polygon points="0 b.y a.x 2*b.y a.x 1 0 1-b.y"/>
		<polygon points="1-a.x 2*b.y 1 b.y 1 1-b.y 1-a.x 1"/>
		<polygon points="a.x 2*b.y 1-a.x 2*b.y 1-a.x 1 a.x 1"/>
	</shape>
