	<shape name="Diamond Gem" winding="nonzero">
		<param name="a"
		       origin-x="0" origin-y="-0.1"
		       min-x="0" min-y="-0.1"
		       def-x="0.2" def-y="-0.1"
		       max-x="0.5" max-y="-0.1"/>
		<param name="b"
		       origin-x="-0.1" origin-y="0"
		       min-x="-0.1" min-y="0"
		       def-x="-0.1" def-y="0.2"
		       max-x="-0.1" max-y="0.5"/>
		<path d="M 0.5 1
		         L 0 b.y
		         L a.x 0
		         L 1-a.x 0
		         L 1 b.y Z"/>
		<path d="M 0.5 1
		         L 1/3 b.y
		         L (1+a.x)/3 0
		         L (2-a.x)/3 0
		         L 2/3 b.y Z"/>
		<line x1="0" y1="b.y" x2="1" y2="b.y"/>
	</shape>
